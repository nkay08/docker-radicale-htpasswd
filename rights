[admin]
user = admin
collection = .*
permission = rw

# view user have only read
[view-read-only]
user = view_.*
collection = ^%(login)s(/.+)?$
permission = r

# read access to root collection for all users
[all-read-root-collection]
user = .+
collection =
permission = r

# write access to own files for each user
[owner-write]
user: .+
collection: ^%(login)s(/.+)?$
permission: rw
